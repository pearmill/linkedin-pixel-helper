{
  "name": "Pearmill Pixel Helper for LinkedIn",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [],
  "web_accessible_resources": [
    "icon-128.png",
    "icon-34.png"
  ],
  "manifest_version": 2,
  "permissions": ["<all_urls>", "webRequest", "webNavigation"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
